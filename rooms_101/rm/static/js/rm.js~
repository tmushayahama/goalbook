$(document).ready(function () {
    console.log("Loading rm.js...");
    $.ajaxSetup({traditional: true});
    $("#rm-login-btn").click(login());
});
function login () {
    $.post('/', $('#rm-login-form').serialize(), function (data) {
        console.log("login processed");
        console.log(data);
    }, 'json');
}
